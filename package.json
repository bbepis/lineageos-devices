{
  "name": "lineageos-devices",
  "version": "1.4.2",
  "description": "My flawless Nuxt.js project",
  "author": "Fabian Bucher",
  "homepage": "https://github.com/bucherfa/lineageos-devices",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "push:gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "deploy": "npm run generate:gh-pages && npm run push:gh-pages",
    "update:data": "node ./setup/gatherData.js",
    "update:images": "node ./setup/generateImages.js",
    "update:wiki": "node ./setup/lineageosWiki.js",
    "update:spreadsheet": "node ./setup/downloadSpreadsheet.js",
    "update": "npm run update:spreadsheet && npm run update:wiki && npm run update:images && npm run update:data"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/dotenv": "^1.4.0",
    "ajv": "^6.12.4",
    "ant-design-vue": "^1.6.4",
    "csvtojson": "^2.0.10",
    "follow-redirects": "^1.14.2",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "nuxt": "^2.14.3",
    "nuxt-ackee": "^2.0.0",
    "replace-ext": "^2.0.0",
    "require-yml": "^2.0.0",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "push-dir": "^0.4.1",
    "node-fetch": "^2.6.1"
  }
}
